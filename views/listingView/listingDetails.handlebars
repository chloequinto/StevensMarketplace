<div class="d-flex flex-column flex-md-row align-items-center p-3 px-md-4 mb-3 bg-white border-bottom box-shadow">
<h5 class="my-0 mr-md-auto font-weight-normal">Stevens Marketplace</h5>

<nav class="my-2 my-md-0 mr-md-3">
    <a class="p-2 text-dark" href="/home">Home</a>
    <a class="p-2 text-dark" href="/newListing">New Listing</a>
    <a class="p-2 text-dark" href="/cart">Cart</a>
    <div class="dropdown">
        <button class="dropbtn"><img src = "../../public/images/profile icon.jpg"  width="25" height="25" alt ="Profile"></button>
        <div class="dropdown-content">
        <a href="/profile">Your profile</a>
        <a href="/yourListings">Your listings</a>
        </div>
    </div>

</nav>
<a class="btn btn-outline-danger" href="/logout">Logout</a>

</div>
<table>
    <tr>
        <td>
            <section>
                <div class="container">
                    <img src="{{productData.image}}" class= "productImage" onerror="this.src='../../public/images/none.png'"/>
                    <p>
                        {{productData.productName}}
                    </p>
                    <p>
                        {{productData.description}}
                    </p>
                    <p> 
                        ${{productData.price}}
                    </p>
                      <a href='' onclick="this.href='/cart?id={{productData._id}}'" class="btn btn-danger" id="likeID">Add to cart</a>
                   
                </div>
            </section>
        </td>

    </tr>
    
</table>

